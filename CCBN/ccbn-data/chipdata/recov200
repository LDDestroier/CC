local stage, players, objects, projectiles, act, images = ...
return {
	info = {
		name = "Recov200",
		description = "Gives you 200 health!",
		cooldown = {
			shoot = 6,
			move = 2
		}
	},
	logic = function(info)
		info.player.health = math.min(info.player.health + 200, info.player.maxHealth)
		return false
	end,
}
