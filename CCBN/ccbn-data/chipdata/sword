local stage, players, objects, projectiles, act, images = ...
return {
	info = {
		name = "Sword",
		description = "Slash forwards 1 panel!",
		cooldown = {
			shoot = 8,
			move = 4
		}
	},
	logic = function(info)

		act.stage.setDamage(info.x + info.direction, info.y, 80, info.owner, 4)

		return false
	end
}